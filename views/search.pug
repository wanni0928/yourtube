extends layouts/main
include mixins/videoBlock

block content
    .search-header
        h3 Searching for #{searchingBy}
    .search-video
    if videos.length === 0
        h5 No Videos Found
    each video in videos
        +videoBlock({
                id:video.id,
                title:video.title,
                views:video.views,
                videoFile:video.videoFile
            })

